{
  "angular.experimental-ivy": true,
  "tsserver.npm": "yarn",
  "tsserver.maxTsServerMemory": 9216,
  "workspace.workspaceFolderCheckCwd": true,
  "eslint.packageManager": "yarn",
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextCurrentLineOnly": false,
  "workspace.workspaceFolderFallbackCwd": false,
  "Lua.workspace.library": {
    "/usr/local/share/nvim/runtime/lua": true,
    "/usr/local/share/nvim/runtime/lua/vim": true,
    "/usr/local/share/nvim/runtime/lua/vim/lsp": true
  },
  "Lua.diagnostics.globals": ["vim"],
  "eslint.filetypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html"
  ],
  "eslint.autoFixOnSave": true,
  "suggest.virtualText": true,
  "snippets.ultisnips.pythonPrompt": false
}
